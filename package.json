{
  "name": "nextjs-fastapi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app --reload",
    "fastapi-new": "uvicorn api.index:app --reload",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run fastapi-new\" \"npm run next-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "8.41.0",
    "eslint-config-next": "^14.2.3",
    "next": "^14.2.3",
    "next-auth": "^4.24.7",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  }
}
